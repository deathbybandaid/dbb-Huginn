{
  "secrets": [
    "xkcdrss"
  ],
  "expected_receive_period_in_days": 2,
  "template": {
    "title": "XKCD comics as a feed",
    "description": "This is a feed of recent XKCD comics, generated by Huginn",
    "item": {
      "title": "{{title}} | {{pubDate}}",
      "description": "<img src=\"{{img}}\"; style=\"max-width: 350px; float:left \">{{title}}",
      "link": "{{url}}",
      "pubDate": "{{pubDate}}",
      "id": "{{url}}"
    },
    "icon": "https://xkcd.com/favicon.ico",
    "link": "https://xkcd.com"
  },
  "ns_media": "true",
  "events_to_show": "9999999"
}
