{
  "secrets": [
    "rssfeed"
  ],
  "expected_receive_period_in_days": 2,
  "template": {
    "title": "Indeed Reviews",
    "description": "This is a feed of recent Indeed Reviews, generated by Huginn",
    "item": {
      "title": "{{stars}} Stars |  {{review_title}} | {{ post_date | date: \"%a, %b %d, %Y\" }}",
      "link": "{{review_url}}",
      "description": "{{job_title}} {% line_break %} {{employee_status}} {% line_break %} {{review_body}}",
      "pubDate": "{{post_date}}",
      "id": "{{review_url}}"
    },
    "link": "https://indeed.com/"
  },
  "ns_media": "true"
}
